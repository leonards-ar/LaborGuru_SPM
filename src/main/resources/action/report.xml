<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="report"  namespace="/report" extends="spm-default">
		<action name="totalHoursReport_*" method="{1}" class="com.laborguru.action.report.TotalHoursReportPrepareAction">
			<result name="input" type="tiles">showWeeklyTotalHours</result>
		</action>
		
		<action name="totalHoursReportByPosition_*" method="{1}" class="com.laborguru.action.report.TotalHoursReportByPositionPrepareAction">
			<result name="input" type="tiles">showWeeklyTotalHoursByPosition</result>
		</action>
	</package>
</struts>
