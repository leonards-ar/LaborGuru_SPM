<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.laborguru.model" schema="spm">
	<class name="Store" table="tbl_stores">
		<id name="id" type="int" unsaved-value="null" column="store_id">
			<generator class="native">
				<param name="sequence">tbl_stores_id_seq</param>
				<param name="parameters">
					increment by 1 start with 1000
				</param>
			</generator>
		</id>
		<property name="name" column="name" type="string" length="150" />
		<many-to-one name="office" class="Office" lazy="false">
			<column name="office_id"/>
		</many-to-one>
	</class>
</hibernate-mapping>